{
  "name": "@ycmd/commands",
  "version": "0.18.1",
  "description": "LSK.js CLI Creds is the easiest way to manage GitHub / Gitlab secrets and credentials",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "         ycmd dev",
    "build": "       ycmd build",
    "build:tsup": "  tsup src --clean && echo \"{\\\"type\\\":\\\"module\\\"}\" > lib/package.json",
    "dev:tsup": "    tsup src --watch",
    "build:swc": "   swc src -d lib && swc src -d cjs -C module.type=commonjs",
    "test": "        ycmd test",
    "prepack": "     ycmd prepack",
    "release": "     ycmd release",
    "start": "       ycmd info",
    "info": "        ycmd info"
  },
  "dependencies": {
    "@lsk4/algos": "^4.3.0",
    "@lsk4/err": "^4.4.0",
    "@lsk4/stringify": "^4.4.0",
    "@macrobe/tsup": "^0.0.1",
    "@size-limit/preset-app": "^11.0.1",
    "@size-limit/preset-big-lib": "^11.0.1",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@ycmd/creds": "^0.18.1",
    "@ycmd/helpers": "^0.18.1",
    "@ycmd/run": "^0.18.1",
    "chokidar": "^3.5.3",
    "clean-publish-fix": "^4.2.1",
    "fishbird": "^1.1.8",
    "gitlog": "^4.0.8",
    "lerna-version": "^6.6.2",
    "license-checker": "^25.0.1",
    "nodemon": "^3.0.2",
    "rsync": "^0.6.1",
    "semver": "^7.5.4",
    "size-limit": "^11.0.1",
    "sort-package-json": "^2.6.0",
    "tsm": "^2.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/yargs": "^17.0.32",
    "ycmd": "^0.18.1"
  },
  "peerDependencies": {
    "ycmd": "*"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    },
    "./presets/tsup.config": {
      "import": "./lib/presets/tsup.config.js",
      "types": "./cjs/presets/tsup.config.d.ts",
      "default": "./cjs/presets/tsup.config.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/libs",
  "homepage": "https://github.com/lskjs/cli",
  "bugs": "https://github.com/lskjs/libs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "lskjs",
    "lsk",
    "cli",
    "helper"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
