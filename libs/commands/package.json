{
  "name": "@ycmd/commands",
  "version": "0.4.0",
  "description": "LSK.js CLI Creds is the easiest way to manage GitHub / Gitlab secrets and credentials",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build:ts --no-dts",
    "build:ts": "       tsup src",
    "build:swc": "      swc src -d lib && swc src -d cjs -C module.type=commonjs",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release",
    "start": "          ycmd info",
    "info": "           ycmd info"
  },
  "dependencies": {
    "@lerna/version": "^6.6.2",
    "@lsk4/algos": "^4.0.0-alpha.1",
    "@lskjs/stringify": "^3.19.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@ycmd/helpers": "^0.4.0",
    "@ycmd/run": "^0.4.0",
    "chokidar": "^3.5.3",
    "clean-publish": "github:isuvorov/clean-publish",
    "fishbird": "^1.0.0",
    "gitlog": "^4.0.8",
    "license-checker": "^25.0.1",
    "size-limit": "^11.0.0",
    "sort-package-json": "^2.6.0",
    "tsup": "^8.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/yargs": "^17.0.31"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./lib/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/libs",
  "homepage": "https://github.com/lskjs/cli",
  "bugs": "https://github.com/lskjs/libs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "lskjs",
    "lsk",
    "cli",
    "helper"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
