{
  "name": "@ycmd/utils",
  "version": "0.9.0",
  "description": "LSK.js CLI Creds is the easiest way to manage GitHub / Gitlab secrets and credentials",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "build:tsup": "     tsup src --clean && echo \"{\\\"type\\\":\\\"module\\\"}\" > lib/package.json",
    "dev:tsup": "       tsup src --watch",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "dependencies": {
    "@lsk4/env": "^4.0.2",
    "@lsk4/err": "^4.0.2",
    "@lsk4/log": "^4.0.2",
    "bundle-require": "^4.0.2",
    "fishbird": "^1.1.8",
    "global-prefix": "^3.0.0",
    "joycon": "^3.1.1",
    "lilconfig": "^3.0.0",
    "strip-json-comments": "^3.1.1"
  },
  "devDependencies": {
    "@types/global-prefix": "^3.0.2",
    "@types/node": "^20.10.3"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/libs",
  "homepage": "https://github.com/lskjs/cli",
  "bugs": "https://github.com/lskjs/libs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "lskjs",
    "lsk",
    "cli",
    "helper"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
