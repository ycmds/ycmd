{
  "name": "ycmd",
  "version": "0.0.2",
  "description": "LSK.js CLI (Command Line Interface) is the easiest way to create and manage LSK.js project",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lsk4/err": "^4.0.0-alpha.0",
    "@ycmd/commands": "workspace:^",
    "@ycmd/helpers": "workspace:^",
    "@ycmd/run": "workspace:^",
    "@ycmd/utils": "workspace:^",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/yargs": "^17.0.32"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "bin",
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "bin": {
    "ycmd": "bin/ycmd"
  },
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./lib/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-cli/lsk",
  "homepage": "https://github.com/lskjs/cli",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "lskjs",
    "lsk",
    "cli",
    "helper"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
